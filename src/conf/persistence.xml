<?xml version="1.0" encoding="UTF-8"?>
<persistence version="2.0" xmlns="http://java.sun.com/xml/ns/persistence" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/persistence http://java.sun.com/xml/ns/persistence/persistence_2_0.xsd">
  <persistence-unit name="ProjetPartenariatsPU" transaction-type="JTA">
    <jta-data-source>jdbc/mineworld</jta-data-source>
    <class>entity.Compte</class>
    <class>entity.Destination</class>
    <class>entity.FichierUploade</class>
    <class>entity.Pays</class>
    <class>entity.Profil</class>
    <class>entity.Rubrique</class>
    <class>entity.Ville</class>
    <exclude-unlisted-classes>false</exclude-unlisted-classes>
    <properties/>
  </persistence-unit>
</persistence>
